my $fh;
my $a = -r *FH;
$a = -w '/some/path/name';
$a = -x $fh;
$a = -o;
$a = -R _;
$a = -W $fh;
$a = -X *FH;
$a = -O '/some/file/name';
$a = -e;
$a = -z _;
$a = -s '/some/file/name';
$a = -f $fh;
$a = -d *FH;
$a = -l;
$a = -p _;
$a = -S *FH;
$a = -b '/some/file/name';
$a = -c $fh;
$a = -t _;
$a = -u;
$a = -g $fh;
$a = -k *FH;
$a = -T '/some/file/name';
$a = -B;
$a = -M _;
$a = -A '/some/file/name';
$a = -C $fh
